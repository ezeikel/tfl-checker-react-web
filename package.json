{
  "name": "tfl-checker-react-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|html|md)\"",
    "lint": "node_modules/.bin/eslint --ext .jsx,.js,.tsx,.ts src/",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --watchAll=false",
    "test:watch": "react-scripts test --env=jsdom",
    "test:coverage": "yarn test --coverage --env=jsdom --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.+(js|jsx|json|ts|tsx|css|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "react-scripts test --env=jsdom --watchAll=false --findRelatedTests"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "valid-typeof": "error",
      "react/react-in-jsx-scope": 0,
      "react/state-in-constructor": 0,
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "react/prop-types": 0,
      "react/require-default-props": 0,
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.test.{js,jsx,ts,tsx}",
            "**/setupTests.js",
            ".storybook/**",
            "stories/**"
          ]
        }
      ],
      "no-console": [
        2,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "react/function-component-definition": [
        0,
        {
          "namedComponents": "function-declaration"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "jsx-a11y/anchor-is-valid": [
            "error",
            {
              "components": [
                "Link"
              ],
              "specialLink": [
                "hrefLeft",
                "hrefRight"
              ],
              "aspects": [
                "invalidHref",
                "preferButton"
              ]
            }
          ]
        }
      }
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "google-map-react": "^1.1.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-places-autocomplete": "^7.3.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.1.1",
    "react-scripts": "^5.0.0",
    "react-slick": "^0.25.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.18",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
